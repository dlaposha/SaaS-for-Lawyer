    "@fullcalendar/timegrid": "^6.1.9",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "antd": "^5.27.4",
    "axios": "^1.7.0",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.18",
    "i18next": "^23.0.0",
    "i18next-browser-languagedetector": "^7.0.0",
    "i18next-http-backend": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^13.0.0",
    "react-router-dom": "^6.0.0",
    "recharts": "^3.2.1",
